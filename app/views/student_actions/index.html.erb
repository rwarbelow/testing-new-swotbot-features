<h1>Listing student_actions</h1>

<table>
  <thead>
    <tr>
      <th>Action type</th>
      <th>Student</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @student_actions.each do |student_action| %>
      <tr>
        <td><%= student_action.action_type_id %></td>
        <td><%= student_action.student_id %></td>
        <td><%= link_to 'Show', student_action %></td>
        <td><%= link_to 'Edit', edit_student_action_path(student_action) %></td>
        <td><%= link_to 'Destroy', student_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student action', new_student_action_path %>
