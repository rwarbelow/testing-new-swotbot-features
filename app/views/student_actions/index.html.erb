<h1>Listing student_actions</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Action type</th>
      <th>Student</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @student_actions.each do |student_action| %>
      <tr>
        <td><%= student_action.created_at.strftime('%m/%e/%y') %></td>
        <td><%= student_action.action_type.name %></td>
        <td><%= student_action.student.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student action', new_student_action_path %>
